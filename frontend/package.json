{
  "name": "netgen-remote-media-bundle",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^12"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && node copyFiles.js",
    "dev": "vue-cli-service build --mode development && node copyFilesDev.js",
    "lint": "vue-cli-service lint",
    "dev-sha": "npm run dev && npm run sha-remote && npm run sha-remote-vendors",
    "sha-remote": "curl -s http://fh-akeneo.dev.php80.sf/bundles/netgenremotemedia/js/remotemedia.js | openssl dgst -sha256 -binary | openssl base64",
    "sha-remote-vendors": "curl -s http://fh-akeneo.dev.php80.sf/bundles/netgenremotemedia/js/remotemedia-vendors.js | openssl dgst -sha256 -binary | openssl base64"
  },
  "dependencies": {
    "@riophae/vue-treeselect": "^0.4.0",
    "axios": "^0.21.1",
    "core-js": "^2.6.5",
    "cropperjs": "^1.5.5",
    "debounce": "^1.2.0",
    "portal-vue": "^2.1.7",
    "pretty-bytes": "^5.6.0",
    "vue": "^2.6.10",
    "vue-select": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  }
}
