{
    "name": "netgen/remote-media-bundle",
    "description": "Remote media core implementation",
    "type": "symfony-bundle",
    "keywords": [
        "remote media",
        "netgen",
        "remote-media-bundle",
        "cloudinary"
    ],
    "homepage": "https://github.com/netgen/NetgenRemoteMediaBundle",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "http://netgen.io",
            "email": "info@netgen.io"
        },
        {
            "name": "Ivan Herak",
            "email": "ivan@netgen.io"
        },
        {
            "name": "Jurica Hladek",
            "email": "jurica@netgen.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "cloudinary/cloudinary_php": "^1.20",
        "symfony/mime": "^4.4 || ^5.4 || ^6.2",
        "symfony/twig-bundle": "^4.4 || ^5.4 || ^6.2",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.2",
        "symfony/proxy-manager-bridge": "^4.4 || ^5.4 || ^6.2",
        "symfony/form": "^4.4 || ^5.4 || ^6.2",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^10.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "matthiasnoback/symfony-config-test": "^4.2",
        "mikey179/vfsstream": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/var-dumper": "^4.4 || ^5.4 || ^6.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpstan/phpstan-symfony": "^1.2",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\RemoteMedia\\": "lib/",
            "Netgen\\Bundle\\RemoteMediaBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\RemoteMedia\\Tests\\": "tests/lib/",
            "Netgen\\Bundle\\RemoteMediaBundle\\Tests\\": "tests/bundle/"
        }
    },
    "config": {
        "allow-plugins": false
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "fix": "@php vendor/bin/php-cs-fixer fix",
        "phpstan": "@php vendor/bin/phpstan analyse -c phpstan.neon --level=1 --ansi lib bundle",
        "phpstan-tests": "@php vendor/bin/phpstan analyse -c phpstan.tests.neon --level=1 --ansi tests"
    },
    "extra": {
        "ezpublish-legacy-dir": "ezpublish_legacy",
        "branch-alias": {
            "dev-3.0": "3.0.x-dev"
        }
    }
}
