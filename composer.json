{
    "name": "netgen/remote-media-bundle",
    "description": "Remote media core implementation",
    "type": "symfony-bundle",
    "keywords": [
        "remote media",
        "netgen",
        "remote-media-bundle",
        "cloudinary"
    ],
    "homepage": "https://github.com/netgen/NetgenRemoteMediaBundle",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "http://netgen.io",
            "email": "info@netgen.io"
        },
        {
            "name": "Ivan Herak",
            "email": "ivan@netgen.io"
        },
        {
            "name": "Jurica Hladek",
            "email": "jurica@netgen.io"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "cloudinary/cloudinary_php": "^1.20",
        "symfony/mime": "^4.4 || ^5.2",
        "symfony/twig-bundle": "^4.4 || ^5.2",
        "symfony/framework-bundle": "^4.4 || ^5.2",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "symfony/proxy-manager-bridge": "^4.4 || ^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "matthiasnoback/symfony-config-test": "^4.2",
        "mikey179/vfsstream": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.0",
        "rector/rector": "^0.11.53"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\RemoteMedia\\": "lib/",
            "Netgen\\Bundle\\RemoteMediaBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\RemoteMedia\\Tests\\": "tests/lib/",
            "Netgen\\Bundle\\RemoteMediaBundle\\Tests\\": "tests/bundle/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "fix": "@php vendor/bin/php-cs-fixer fix"
    }
}
